<is-land on:visible :import="`/js/components/speedlify-score.js?v=${this.pkg.version}`">
	<speedlify-score webc:keep :class="this.class" :hash="this.hash" speedlify-url="https://speedlify.chrisburnell.com" :score="this.score" :weight="this.weight" :requests="this.requests" :rank="this.rank" :rank-change="this.rankChange"></speedlify-score>

	<template data-island="once">
		<link webc:keep rel="stylesheet" :href="`/css/components/speedlify-score.css?v=${this.pkg.version}`" />
	</template>
</is-land>
