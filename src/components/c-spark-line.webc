<is-land class="[ spark-line ] [ pentatonic ]" on:visible>
	<spark-line webc:root="override" :values="this.values" :original="this.original" :curve="this.curve" :color="this.color" :colors="this.colors" :endpoint-color="this['endpoint-color'] || `#e0151f`" :points="this.points" :line-width="this['line-width'] || 1.5" :start-label="this['start-label']" :end-label="this['end-label']" :key-start="this['key-start'] || 25" :key-intervals="this['key-intervals'] || `2,1,2,2,1,2,2`" :key-limit="this['key-limit'] || 15" :id="this.id" title="Click to hear me!"></spark-line>

	<template webc:raw data-island="once">
		<script type="module" src="/js/components/spark-line.js"></script>
	</template>
</is-land>