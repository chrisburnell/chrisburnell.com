<h2>Latest projects</h2>
<ul class=" [ grid ] [ small ] " style="--min-inline-size: 10em; --row-gap: 0;">
	{%- set count = 0 -%}
	{% for lookup in collections.projects %}
	{% if page.url != lookup.url and count < site.limits.feature %}
	<li>
		<a href="{{ lookup.url }}" class=" [ riza ] "><c-emoji>{{ lookup.data.emoji }}</c-emoji>{{ lookup.data.title | striptags(true) | safe }}</a>
		{{ lookup.data.description | formatAsMarkdown | safe }}
	</li>
	{%- set count = count + 1 -%}
	{% endif %}
	{% endfor %}
</ul>
<p><a href="/projects/" class=" [ small ] ">Browse Projects â†’</a></p>
