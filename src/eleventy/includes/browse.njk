{%- if categoryCollectionName -%}
	{% set previous_post = collections[categoryCollectionName] | arrayKeyIncludes('data.tags', 'post') | arePublished | noRSSOnlyFilter | getPreviousCollectionItem(page) -%}
	{% set next_post = collections[categoryCollectionName] | arrayKeyIncludes('data.tags', 'post') | arePublished | noRSSOnlyFilter | getNextCollectionItem(page) -%}

	{% if previous_post or next_post %}
		<nav class=" [ browse ] " aria-label="Browse to Previous and Next Posts">
			<ol class=" [ browse__list ] ">
				{% if previous_post %}
					<li>
						<a href="{{ previous_post.url }}" class=" [ browse__previous ] ">
							<strong>← Older</strong>
							<br>
							{% if previous_post.data.title or previous_post.data.of_title or previous_post.data.of_title_fallback -%}
								{{ previous_post.data.title | default(previous_post.data.of_title) | default(previous_post.data.of_title_fallback) | default(previous_post.data.meta_title) | default('Untitled') | striptags(true) | safe }}
							{%- elif previous_post.data.rsvp -%}
								RSVP: {{ previous_post.data.reply_title }}
							{%- else -%}
								Untitled {{ (previous_post.data.rsvp and 'RSVP' or previous_post.data.in_reply_to and 'Reply' or previous_post.data.category and (previous_post.data.categoryProper | default(previous_post.data.category) | supertitle) or 'Page') }}
							{%- endif %}
						</a>
					</li>
				{% endif %}
				{% if next_post %}
					<li>
						<a href="{{ next_post.url }}" class=" [ browse__next ] ">
							<strong>Newer →</strong>
							<br>
							{% if next_post.data.title or next_post.data.of_title or next_post.data.of_title_fallback -%}
								{{ next_post.data.title | default(next_post.data.of_title) | default(next_post.data.of_title_fallback) | default(next_post.data.meta_title) | striptags(true) | safe }}
							{%- elif next_post.data.rsvp -%}
								RSVP: {{ next_post.data.reply_title }}
							{%- else -%}
								Untitled {{ next_post.data.rsvp and 'RSVP' or next_post.data.in_reply_to and 'Reply' or next_post.data.category and (next_post.data.categoryProper | default(next_post.data.category) | supertitle) or 'Page' }}
							{%- endif %}
						</a>
					</li>
				{% endif %}
			</ol>
		</nav>
	{% endif %}
{%- endif -%}
