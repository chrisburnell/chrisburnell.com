    "rollup-plugin-terser": "7.0.2",
    "sass": "^1.69.7",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.0.0-alpha.4",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-plugin-bundle": "^1.0.5",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.3",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@11ty/is-land": "^4.0.0",
    "@chrisburnell/bowhead": "^0.3.4",
    "@chrisburnell/eleventy-cache-webmentions": "^1.2.6-rc.2",
    "@chrisburnell/pentatonic": "^1.1.4",
    "@chrisburnell/spark-line": "^1.2.6",
    "@chrisburnell/theme-selector": "^1.0.1",
    "@daviddarnes/share-button": "^1.1.2",
    "@daviddarnes/storage-form": "^2.0.0",
    "@mdn/browser-compat-data": "^5.5.5",
    "@sindresorhus/slugify": "1.1.2",
    "@tweetback/canonical": "^2.0.38",
    "@zachleat/details-utils": "^2.0.2",
    "@zachleat/pagefind-search": "^1.0.3",
    "@zachleat/seven-minute-tabs": "^3.0.0",
    "@zachleat/table-saw": "^1.0.6",
    "caniuse-api": "^3.0.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "glob-promise": "^6.0.5",
    "html-minifier": "^4.0.0",
    "lite-youtube-embed": "^0.3.0",
    "luxon": "^3.4.4",
    "markdown-it": "^14.0.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-footnote": "^4.0.0",
    "pagefind": "^1.0.4"
  },
  "scripts": {
    "clean": "rm -rf .cache && rm -rf _site",
    "tokens": "npx json-to-scss './src/data/designTokens/*.json' ./src/css/abstracts/_generated.scss; npx prettier --write src/css/abstracts/_generated.scss",
    "css": "npx sass src/css:css --style=compressed --no-source-map --color",
    "postcss": "npx postcss css/*.css --replace --no-map",
    "js": "npx rollup -c",
    "prettier": "npx prettier --write src",
    "start": "concurrently \"npm run css -- --watch\" \"NODE_OPTIONS=--max-old-space-size=8192 npx @11ty/eleventy --incremental --serve\"",
    "now": "npx @11ty/eleventy --incremental --ignore-initial --serve",
    "serve": "NODE_OPTIONS=--max-old-space-size=8192 npx @11ty/eleventy --incremental --serve",
    "setup": "npm run tokens; npm run css; npm run js; npm run prettier; npx browserslist@latest --update-db",
    "build": "npm run setup; cross-env NODE_OPTIONS=--max-old-space-size=8192 ELEVENTY_PRODUCTION=true npx @11ty/eleventy",
    "create-search-index": "npx pagefind --site \"_site\"",
    "pregenerate": "npm run setup; cross-env PREGENERATE_IMAGES=true npx @11ty/eleventy",
    "debug": "NODE_OPTIONS=--max-old-space-size=8192 cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
  },
  "imports": {
    "#builders/*": "./src/eleventy/collectionBuilders/*.js",
    "#data/*": "./src/data/*.json",
    "#datajs/*": "./src/data/*.js",
    "#eleventy/*": "./src/eleventy/*.js",
    "#filters/*": "./src/eleventy/filters/*.js",
    "#plugins/*": "./src/eleventy/plugins/*.js",
    "#transforms/*": "./src/eleventy/transforms/*.js"
  },
  "cssnano": {
    "preset": [
      "default",
      {
        "calc": false
      }
    ]
  }
}
