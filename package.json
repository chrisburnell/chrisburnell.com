  "private": true,
  "main": "index.html",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "npx @11ty/eleventy",
    "build:dev": "npm run setup && cross-env DEV_MODE=true GENERATE_OG_IMAGES=true npx @11ty/eleventy",
    "build:live": "npm run setup && cross-env GENERATE_OG_IMAGES=true npx @11ty/eleventy",
    "css": "npx sass src/css:_site/css --style=expanded --no-source-map --color --quiet",
    "postcss": "npx postcss \"_site/css/**/*.css\" --replace --no-map",
    "check": "node cli/index.js check",
    "check:css": "node cli/index.js calculateCSSComplexity",
    "check:datetimes": "node cli/index.js checkDates",
    "check:links": "node cli/index.js checkLinks",
    "checks": "npm run check",
    "clean": "npx rimraf .cache && npx rimraf _site",
    "cli": "node cli/index.js wizard",
    "create-search-index": "npx pagefind --site \"_site\" --glob \"**/*.html\"",
    "debug": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "lint": "npx eslint \"src/**/*.js\"",
    "now": "npx @11ty/eleventy --ignore-initial --incremental --serve",
    "pregenerate": "npm run setup && cross-env PREGENERATE_IMAGES=true npx @11ty/eleventy",
    "prettier": "npx prettier --write --list-different src",
    "setup": "npm run tokens && npm run prettier && npx update-browserslist-db@latest && npm run css",
    "start": "npm run css && npx @11ty/eleventy --incremental --serve",
    "test": "npx vitest run",
    "tokens": "npx json-to-scss './src/eleventy/data/designTokens/*.json' ./src/css/generated/_tokens.scss && npx prettier --write src/css/generated/_tokens.scss",
    "wizard": "npm run cli"
  },
  "keywords": [],
