    "autoprefixer": "^10.4.17",
    "caniuse-api": "^3.0.0",
    "cheerio": "^1.0.0-rc.12",
    "chrisburnell": "^33.0.6",
    "eleventy-plugin-og-image": "^3.1.0",
    "fast-glob": "^3.3.2",
    "html-minifier": "^4.0.0",
    "lite-youtube-embed": "^0.3.0",
    "luxon": "^3.4.4",
    "markdown-it": "^14.0.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-footnote": "^4.0.0",
    "pagefind": "^1.0.4",
    "postcss": "^8.4.35",
    "utopia-core-scss": "^1.0.0"
  },
  "scripts": {
    "clean": "npx rimraf .cache && npx rimraf _site",
    "tokens": "npx json-to-scss './src/eleventy/data/designTokens/*.json' ./src/css/abstracts/_generated.scss; npx prettier --write src/css/abstracts/_generated.scss",
    "prettier": "npx prettier --write src",
    "check-links": "npx check-html-links _site",
    "create-search-index": "npx pagefind --site \"_site\" --glob \"**/*.html\"",
    "start": "NODE_OPTIONS=--max-old-space-size=8192 npx @11ty/eleventy --incremental --serve",
    "serve": "npm run start",
    "now": "NODE_OPTIONS=--max-old-space-size=8192 npx @11ty/eleventy --incremental --ignore-initial --serve",
    "setup": "npm run tokens; npm run prettier; npx browserslist@latest --update-db",
    "build": "npm run setup; cross-env NODE_OPTIONS=--max-old-space-size=8192 npx @11ty/eleventy; npm run create-search-index",
    "pregenerate": "npm run setup; cross-env NODE_OPTIONS=--max-old-space-size=8192 PREGENERATE_IMAGES=true npx @11ty/eleventy",
    "debug": "cross-env NODE_OPTIONS=--max-old-space-size=8192 DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
  }
}
