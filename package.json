    "@rollup/plugin-node-resolve": "^15.2.3",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.3",
    "json-to-scss": "^1.6.2",
    "postcss-cli": "^11.0.0",
