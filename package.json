    "@daviddarnes/storage-form": "^2.0.0",
    "@mdn/browser-compat-data": "^5.5.13",
    "@oddbird/popover-polyfill": "^0.4.0",
    "@remy/webmention": "^1.5.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@tweetback/canonical": "^2.0.40",
    "@zachleat/details-utils": "^2.0.2",
    "@zachleat/pagefind-search": "^1.0.3",
    "@zachleat/webcare-webshare": "^1.0.3",
    "autoprefixer": "^10.4.18",
    "browserslist": "^4.23.0",
    "caniuse-api": "^3.0.0",
    "cheerio": "^1.0.0-rc.12",
    "chrisburnell": "^33.0.9",
    "dotenv": "^16.4.5",
    "download-stats": "^0.3.4",
    "eleventy-plugin-og-image": "^3.2.0-beta.4",
    "esbuild": "^0.20.1",
    "fast-glob": "^3.3.2",
    "fast-xml-parser": "^4.3.5",
    "fs-extra": "^11.2.0",
    "html-entities": "^2.5.2",
    "html-minifier": "^4.0.0",
    "lite-youtube-embed": "^0.3.1",
    "luxon": "^3.4.4",
    "markdown-it": "^14.0.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-footnote": "^4.0.0",
    "pagefind": "^1.0.4",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "postcss-scss": "^4.0.9",
    "random-case": "^1.0.0",
    "sass": "^1.71.1",
    "striptags": "^3.2.0",
    "truncate-html": "^1.1.1",
    "utopia-core-scss": "^1.0.0"
  },
  "scripts": {
    "clean": "npx rimraf .cache && npx rimraf _site",
    "check-links": "npx check-html-links _site",
    "lint": "npx eslint src/**/*.js",
    "prettier": "npx prettier --write src",
    "tokens": "npx json-to-scss './src/eleventy/data/designTokens/*.json' ./src/css/abstracts/_generated.scss; npx prettier --write src/css/abstracts/_generated.scss",
    "css": "npx sass src/css:_site/css --style=compressed --no-source-map --color",
    "postcss": "npx postcss _site/css/**/*.css --replace --no-map",
    "create-search-index": "npx pagefind --site \"_site\" --glob \"**/*.html\"",
    "debug": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=8192 npx @11ty/eleventy --incremental --serve",
    "now": "cross-env NODE_OPTIONS=--max-old-space-size=8192 npx @11ty/eleventy --ignore-initial --incremental --serve",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 npx @11ty/eleventy",
    "setup": "npm run tokens; npm run prettier; npx browserslist@latest --update-db; npm run css",
    "build-production": "npm run setup; cross-env NODE_OPTIONS=--max-old-space-size=8192 npx @11ty/eleventy; npm run create-search-index",
    "pregenerate": "npm run setup; cross-env NODE_OPTIONS=--max-old-space-size=8192 PREGENERATE_IMAGES=true npx @11ty/eleventy"
  }
}
